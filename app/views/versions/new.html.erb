
<% form_for(:child, :url => item_versions_url(@item), :html => { :multipart => true, :class => "cmxform" }) do |f| %>

  <%= error_messages_for :child %>

  <%= render :partial => 'form', :locals => {:f => f} %>
  
  <p>
    <%= submit_tag "Create a new version", :class => "button positive add" %>
  </p>
<% end %>